local library = loadstring(game:HttpGet(('https://raw.githubusercontent.com/AikaV3rm/UiLib/master/Lib.lua')))()

getgenv().AUTOFARM = false;
getgenv().ANTIBAN = false;
getgenv().STAMINA = false;
getgenv().MakeSomePuzzles = false;

local w = library:CreateWindow("BEAR (Alpha)") -- Creates the window

local a = w:CreateFolder("LocalPlayer") -- Creates the folder(U will put here your buttons,etc)

local EITA = w:CreateFolder("Puzzles") -- Creates the folder(U will put here your buttons,etc)

local F = w:CreateFolder("Auto Farm") -- Creates the folder(U will put here your buttons,etc)

local b = w:CreateFolder("Troll") -- Creates the folder(U will put here your buttons,etc)

local c = w:CreateFolder("Credits") -- Creates the folder(U will put here your buttons,etc)


F:Label("The 1 Is Better",{
    TextSize = 25; -- Self Explaining
    TextColor = Color3.fromRGB(255,255,255); -- Self Explaining
    BgColor = Color3.fromRGB(69,69,69); -- Self Explaining
    
}) 


c:Label("Made by yDex#9119",{
    TextSize = 20;
    TextColor = Color3.fromRGB(55,255,255);
        BgColor = Color3.fromRGB(69,69,69); -- Self Explaining
    
}) 

a:Button("Teleport To Quidz",function()
local p1 = game.Players.LocalPlayer.Character.HumanoidRootPart
local pos = p1.CFrame

p1.CFrame = game.Workspace._Obby.Coinage.CFrame
end)

a:Button("Unlock Camera",function()
game.Players.LocalPlayer.CameraMaxZoomDistance = "1000"
end)

a:Slider("Walkspeed",{
    min = 10; -- min value of the slider
    max = 250; -- max value of the slider
    precise = true; -- max 2 decimals
},function(value)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = (value)
end)
a:Slider("Jump Power",{
    min = 10; -- min value of the slider
    max = 250; -- max value of the slider
    precise = true; -- max 2 decimals
},function(value)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = (value)
end)

a:Button("Remove Effects",function()
game.Lighting.OutdoorAmbient = Color3.fromRGB(260, 260, 260)
game.Lighting.FogEnd = 100000000000000000000000
game.Lighting.Ambient = Color3.fromRGB(260, 260, 260)
game.Lighting.ColorCorrection.Enabled = false
end)

a:Button("Restore Effects",function()
if game.Lighting.ClockTime == 0 then
game.Lighting.OutdoorAmbient = Color3.fromRGB(128, 128, 128)
game.Lighting.FogEnd = 250
game.Lighting.FogColor = Color3.fromRGB(0, 0, 0)
game.Lighting.Ambient = Color3.fromRGB(30, 30, 30)
game.Lighting.ColorCorrection.Enabled = true
end
if game.Lighting.ClockTime == 14 then
game.Lighting.OutdoorAmbient = Color3.fromRGB(30, 30, 30)
game.Lighting.FogEnd = 1000000000
game.Lighting.Ambient = Color3.fromRGB(0, 0, 0)
game.Lighting.ColorCorrection.Enabled = true
end
end)

a:Button("Unlock PlayerList",function()
while true do
wait(.1)

game.StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.PlayerList,true)

end
end)

a:Button("Unlock Reset",function()
while true do
wait(.1)

local StarterGui = game:GetService('StarterGui')

wait()

StarterGui:SetCore("ResetButtonCallback",true)

end
end)
EITA:Button("Goto Colors",function()
local CHEESE = game:GetService("Workspace").RoundArea["_Entities"].Color.Clue.CFrame
local p1 = game.Players.LocalPlayer.Character.HumanoidRootPart
local pos = p1.CFrame
p1.CFrame = CHEESE
end)
EITA:Button("Goto Color Puzzle",function()
local CHEESE = game:GetService("Workspace").RoundArea["_Entities"].Color.Buttons["2"].CFrame
local p1 = game.Players.LocalPlayer.Character.HumanoidRootPart
local pos = p1.CFrame
p1.CFrame = CHEESE
end)
EITA:Button("  Make Cheese Puzzle",function()
local CHEESE = game:GetService("Workspace").RoundArea["_Entities"].Cheese.Cheese.CFrame
local SAVED = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame
local STAND = game:GetService("Workspace").RoundArea["_Entities"].CheeseAltar.Detect.CFrame

local p1 = game.Players.LocalPlayer.Character.HumanoidRootPart
local pos = p1.CFrame
p1.CFrame = CHEESE
wait(0.5)
game.Players.LocalPlayer.Backpack.PuzzleCheese.Parent = game.Players.LocalPlayer.Character
wait(0.1)
p1.CFrame = STAND
wait(0.4)
p1.CFrame = SAVED

end)

b:Button("Get Cheese",function()
local CHEESE = game:GetService("Workspace").RoundArea["_Entities"].Cheese.Cheese.CFrame
local SAVED = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame

local p1 = game.Players.LocalPlayer.Character.HumanoidRootPart
local pos = p1.CFrame
p1.CFrame = CHEESE
wait(0.1)
p1.CFrame = SAVED
end)


