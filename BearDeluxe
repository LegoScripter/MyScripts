F:Toggle("Auto Farm 1",function(bool)
    getgenv().AUTOFARM = bool
    if bool then
    AUTOF();
   end
end)

F:Toggle("Auto Farm 2",function(bool)
    getgenv().ANTIBAN = bool
    if bool then
    AUTOF1();
   end
end)

a:Button("Spectate Gui",function()
game:GetService("Players")["kapanda_OFC"].PlayerGui.Main.LobbyUI.SpectateButton.Visible = true
end)

function AUTOF()
      spawn(function()
          while getgenv().AUTOFARM == true do
          wait(0.1)
          local x = game.Players.LocalPlayer.Character:FindFirstChild("Humanoid")
           if x then
              x.Health = 0
              wait(0.1)
              local p1 = game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
              if p1 then
              local pos = p1.CFrame

              p1.CFrame = game.Workspace._Obby.Coinage.CFrame
              wait(0.25)
              game.Players.LocalPlayer.Character:Destroy() --Make Legit
              wait(6)
              end
          else
             wait(0.1)
              local p1 = game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
              if p1 then
              local pos = p1.CFrame

              p1.CFrame = game.Workspace._Obby.Coinage.CFrame
              wait(0.25)
              game.Players.LocalPlayer.Character:Destroy() --Make Legit
              wait(6)
              end
          end
       end
   end)
end


function AUTOF1()
      spawn(function()
          while getgenv().ANTIBAN == true do
          local p1 = game.Players.LocalPlayer.Character.HumanoidRootPart
          local pos = p1.CFrame

          p1.CFrame = game.Workspace._Obby.Coinage.CFrame
          wait(0.75)
          game.Players.LocalPlayer.Character.Humanoid.Jump = true
          wait(1)
          game.Players.LocalPlayer.Character.Humanoid.Jump = true
          wait(7)
       end
   end)
end

function doTapXD()
      spawn(function()
          while getgenv().STAMINA == true do
          wait(0.00000000001)
          game:GetService("Players").LocalPlayer.PlayerGui.GameUI.InGameDisplay.StaminaDisplay.Current.Value = 100
       end
   end)
end
